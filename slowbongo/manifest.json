{
  "id": "slowbongo",
  "name": "Slow Bongo",
  "version": "0.7.8",
  "minNoctaliaVersion": "3.6.0",
  "author": "tui",
  "official": false,
  "description": "A bongo cat that sits in your bar and slaps when you type.",
  "license": "MIT",
  "repository": "https://github.com/tui/slowbongo",
  "tags": [
    "Bar",
    "Cute",
    "Audio",
    "Fun"],
  "entryPoints": {
    "main": "Main.qml",
    "barWidget": "BarWidget.qml",
    "settings": "Settings.qml"
  },
  "dependencies": {
    "plugins": []
  },
  "metadata": {
    "defaultSettings": {
      "idleTimeout": 150,
      "waitingTimeout": 30000,
      "catColor": "default",
      "catSize": 1.0,
      "catOffsetY": 0.0,
      "raveMode": false,
      "tappyMode": false,
      "useMprisFilter": false
    }
  }
}
